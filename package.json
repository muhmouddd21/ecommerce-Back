{
  "name": "self_project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy:migrations",
    "copy:migrations": "mkdir -p dist/datastore/sqlDB && cp -R datastore/sqlDB/migrations dist/datastore/sqlDB/",
      "verify:migrations": "node -e \"const fs = require('fs'); console.log('Migration files:', fs.readdirSync('dist/datastore/sqlDB/migrations', {withFileTypes: true}).map(d => d.name));\"",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node index.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.5.6",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.7",
    "copyfiles": "^2.4.1",
